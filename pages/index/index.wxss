/**index.wxss**/
.index-lun{
   height:400rpx;
}
.slide-image{
   width: 100%;
   height: 100%;
}

.nav-box{
    padding:30rpx 0 0rpx 0;
    background:white;
}
.nav-box .nav_list{
    width:25%;
    float: left;
    height:200rpx;
    text-align:center;
}
.nav-box .nav_list .img {
    width:55%;
    height:52%;
    text-align:center;
    margin:0 auto;
}
.nav-box .nav_list image {
    width:100%;
    height:100%;
    border-radius:50%;
}
.list {
    color:#000;
    font-size:29rpx;
    margin-top:15rpx;
}

.tjsp{
  margin-bottom: 15rpx; 
  background-color: white;
  margin: 20rpx auto;
}
.rmwx{
   margin-bottom: 15rpx; 
  background-color: white;
  margin: 20rpx auto;
   padding-bottom: 15rpx;
}
.rmwx-top,.tjsp-top{
  width: 92%;
  margin:0 auto;
  padding: 20rpx 0;
  height: 35rpx;
}
.tjsp-top{
  margin-bottom: 15rpx;
}
.tjsp-top image,.rmwx-top image{
  height: 100%;
  width: 1%;
  vertical-align: middle;
}
.tjsp-top text, .rmwx-top text{
  font-size: 33rpx;
  margin-left: 10rpx;
}
.rmwx-text{
  width: 92%;
  margin:10rpx auto;
  border-top: 1rpx solid #e0e0e0;
}
.rmwx-text text{
  font-size: 30rpx;
  width: 24.7%;
   padding:20rpx 0; 
  float: left;
  display: block;
  text-align: center;
  color: #666666;
}
.rmwx-text text:nth-child(1),.rmwx-text text:nth-child(2),.rmwx-text text:nth-child(3),.rmwx-text text:nth-child(4){
  border-bottom: 1rpx solid #e0e0e0;
}
.rmwx-text navigator:nth-child(2) text,.rmwx-text navigator:nth-child(3) text,.rmwx-text navigator:nth-child(4) text,.rmwx-text navigator:nth-child(6) text,.rmwx-text navigator:nth-child(7) text,.rmwx-text navigator:nth-child(8) text{
  border-left:1rpx solid #e0e0e0;
}

page{
  padding-bottom: 120rpx;
} 
.tjsp-top navigator{
  width: 30%;
  float: right;
  text-align: right;
  font-size: 40rpx;
  color: #c4c4c4;
}
.tjsp-top navigator text{
  font-size: 30rpx;
   margin-bottom: 25rpx; 
}
.tjsp-bottom{
  background-color: #f4f4f4;
}
.tjsp-bottom-all{
  width: 92%;
  margin: auto;
}
.red{
  padding: 5rpx 0;
  font-size: 26rpx;
  color: #ff0000;
}
.tjsp-li{
  width: 48%;
  float: left;
  background-color: white;
  margin-bottom: 25rpx;
}
.tjsp-li:nth-child(odd){
  margin-right: 4%;
}
.tjsp-li image{
  width: 100%;
  height:330rpx;
}
.tjsp_price{
  padding-left: 10rpx;
  font-size: 29rpx;
}
.tjsp_price>text{
  padding: 8rpx 0;
}

.modal_screen { 
 width: 100%; 
 height: 100%; 
 position: fixed; 
 top: 0; 
 left: 0; 
 z-index: 1000; 
 background: #000; 
 opacity: 0.5; 
} 

.modal_box { 
 width: 86%; 
 height:550rpx;
 position: fixed; 
 top: 8%; 
 left:7%; 
 z-index: 1001; 
 border-radius:10rpx; 
} 
.modal_quanxian{
  width:100%;
  height:250rpx;
}
.modal_quanxian image{
  width: 100%;
  height: 102%;
}
.quan_div{
  width: 100%;
  padding: 30rpx 0;
  height: 550rpx;
  background-color: #51cc00;
  border-radius: 0 0 40rpx 40rpx;
  overflow-y: scroll;
}
.quan_div::-webkit-scrollbar{
  display: none;
}
.yhq{
  width: 85%;
  margin:0 auto 13rpx auto;
  height: 160rpx;
  position: relative;
}
.yhq image{
  height: 100%;
  width: 100%;
}
.yhq-left{
  width: 30%;
  float: left;
  position: absolute;
  top: 0;
  left: 0;
  color: #d02b28;
  font-size: 24rpx;
  text-align: center;
  line-height: 160rpx;
}
.yhq-right{
  width: 70%;
  float: left;
  position: absolute;
  top: 0;
  left: 32%;
}
.yhq-left .jiage{
  display: inline-block;
  font-size:68rpx;
}
.yr-left{
  float: left;
  width: 63%;
  margin: 25rpx 0;
}
.yr-1{
  font-size: 30rpx;
  color: #767676;
}
.yr-2{
  font-size: 25rpx;
  color: #b9b9b9;
}
.yr-3{
  font-size: 20rpx;
  color: #e3e3e3;
}
.yr-button{
  font-size: 24rpx;
  background-color: #6aaf3a;
  color: white;
  float: left;
  width: 27%;
  height: 50rpx;
  line-height: 48rpx;
  text-align: center;
  border-radius: 10rpx;
  margin: 13% 0;
}
.close{
  width:90rpx;
  height: 90rpx;
  margin:30rpx auto;
}
.close image{
  width: 95%;
  height: 95%;
  margin: auto;
}
